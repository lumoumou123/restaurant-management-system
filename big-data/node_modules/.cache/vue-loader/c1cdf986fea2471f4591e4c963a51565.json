n = false\" @logged-in=\"setUserRole\" />\n\n    <!-- 主界面 -->\n    <div class=\"main-container\">\n      <router-view />\n    </div>\n  </div>\n</template>\n\n<script>\nimport Login from \"./views/Login.vue\";\n\nexport default {\n  components: { Login },\n  data() {\n    return {\n      userRole: null, // 存储用户角色\n      showLogin: false\n    };\n  },\n  methods: {\n    setUserRole(role) {\n      this.userRole = role;\n      this.showLogin = false;\n    },\n    logout() {\n      localStorage.removeItem(\"user\");\n      this.userRole = null;\n    }\n  },\n  mounted() {\n    const user = JSON.parse(localStorage.getItem(\"user\"));\n    if (user) {\n      this.userRole = user.role;\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* 导航栏 */\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background: #222;\n  color: white;\n  padding: 15px 20px;\n  font-size: 18px;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n}\n\n.nav-actions {\n  display: flex;\n  align-items: center;\n}\n\n.login-btn, .logout-btn {\n  background: #007bff;\n  color: white;\n  border: none;\n  padding: 8px 15px;\n  border-radius: 5px;\n  cursor: pointer;\n  margin-left: 10px;\n  transition: 0.3s;\n}\n\n.login-btn:hover, .logout-btn:hover {\n  background: #0056b3;\n}\n\n/* 页面主体 */\n.main-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  height: calc(100vh - 80px); /* 保持内容居中 */\n}\n</style>\n"]}]}